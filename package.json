    "identifier",
    "curly brackets",
    "pandoc",
    "header_attributes"
  ],
  "dependencies": {},
  "devDependencies": {
    "browserify": "14.3.0",
    "eslint": "3.19.0",
    "markdown-it": "github:GerHobbelt/markdown-it",
    "mocha": "3.3.0"
  },
  "peerDependencies": {
    "markdown-it": "github:GerHobbelt/markdown-it"
  },
  "tonicExampleFilename": "demo.js"
