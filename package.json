{
  "name": "markdown-it-attrs",
  "version": "0.6.4",
  "description": "Add classes, identifiers and attributes to your markdown with {} curly brackets, similar to pandoc's header attributes",
  "main": "index.js",
  "license": "MIT",
  "repository": "arve0/markdown-it-attrs",
  "author": {
    "name": "Arve Seljebu",
    "email": "arve.seljebu@gmail.com",
    "url": "arve0.github.io"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "browser": "browserify index.js --standalone markdown-it-attrs -o markdown-it-attrs.browser.js",
    "build": "npm run browser",
    "lint": "eslint .",
    "preversion": "npm test && npm run browser && git add markdown-it-attrs.browser.js",
    "postpublish": "git push && git push --tags",
    "test": "mocha"
  },
  "homepage": "https://github.com/arve0/markdown-it-attrs",
  "keywords": [
    "markdown-it",
